[req]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[dn]
C  = CA
ST = ON
CN = Impressive Client

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1   = localhost
DNS.2   = pranberry.local
IP.1    = 127.0.0.1
IP.2    = 192.168.68.70